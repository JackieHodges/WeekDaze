<form class="form newjob-form">
    <h2>New Job:</h2>
    <div class="form-group">
        <label for="job-title">Title:</label>
        <input class="form-input" type="text" id="job-title-input" />
    </div>
    <div class="form-group">
        <label for="company">Company:</label>
        <input class="form-input" type="text" id="company-input" />
    </div>
    <div class="form-group">
        <label for="status">Status:</label>
        <input class="form-input" type="text" id="status-input" />
    </div>
    <div class="form-group">
        <a class="btn btn-primary btn-form-submit" type="submit">Submit</a>
    </div>
</form>

<script src="./js/newJob.js"></script>